record(ai, "BLD:SYS0:$(LOCA):PCAV_CHARGE1") {
  field(DESC, "Phase Cavity Charge1")
  field(SCAN, "$(DIAG_SCAN)")
  field(DTYP, "BLD_PCAV")
  field(INP,  "@PCAV_CHARGE1")
  field(PREC, "5")
  field(TSE,  "-1")
  field(EGU,  "nC")
  field(FLNK, "BLD:SYS0:$(LOCA):EFPCAV_CHARGE1")
  field(PRIO, "HIGH")
}

