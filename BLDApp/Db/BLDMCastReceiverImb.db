record(ai, "BLD:SYS0:$(LOCA):IMB_SUM") {
  field(DESC, "Imb Sum")
  field(SCAN, "$(DIAG_SCAN)")
  field(DTYP, "BLD_IMB")
  field(INP,  "@IMB_SUM")
  field(PREC, "5")
  field(TSE,  "-2")
  field(EGU,  "")
  field(FLNK, "BLD:SYS0:$(LOCA):EFIMB_SUM")
  field(PRIO, "HIGH")
}

