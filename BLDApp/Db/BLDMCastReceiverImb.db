record(longin, "BLD:SYS0:$(LOCA):IMB_LATMAX") {
	field(DESC, "Max. IMB BLD Latency")
	field(EGU,  "uS")
	field(SCAN, "$(STAT_SCAN)")
	field(DTYP, "BusAddress")
	field(INP,  "#C0S2@imb_latmax_f,m32")
}

# just write .PROC
record(longout, "BLD:SYS0:$(LOCA):IMB_LATMAXRST") {
	field(DESC, "Rst. Max. IMB BLD Latency")
	field(SCAN, "Passive")
	field(DTYP, "BusAddress")
	field(OUT,  "#C0S2@imb_latmax_f,m32")
	field(VAL,  "0")
	field(PINI, "YES")
}

