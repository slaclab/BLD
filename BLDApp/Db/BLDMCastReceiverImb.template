record(ai, "$(DEVICE):IMB_$(STN)_PULSEID") {
  field(DESC, "Imb PULSEID")
  field(SCAN, "I/O Intr")
  field(DTYP, "BLD_RCVR")
  field(INP,  "@0 $(MC)")
  field(PREC, "5")
  field(TSE,  "-2")
  field(EGU,  "Count")
  field(FLNK, "$(DEVICE):EFIMB$(MC)_PULSEID")
  field(PRIO, "HIGH")
}

